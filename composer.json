{
    "name": "zendaemon/service-layer",
    "description": "Service layer for laravel applications",
    "license": "MIT",
    "authors": [
        {
            "name": "zendaemon",
            "email": "denis.olenin@outlook.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.2",
        "illuminate/support": "^5|^6"
    },
    "require-dev": {
        "orchestra/testbench": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "Zendaemon\\Services\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zendaemon\\Services\\Tests": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zendaemon\\Services\\ServicesServiceProvider"
            ]
        }
    }
}
